{#
    Main CSS can be found in metadata.twig
#}

{# Core Theme Components #}
<script src="{{ asset_path("theme::js/vendor/jquery.min.js") }}"></script>
<script src="{{ asset_path("theme::js/vendor/bootstrap.min.js") }}"></script>
<script src="{{ asset_path("theme::js/vendor/vue.min.js") }}"></script>
<script src="{{ asset_path("theme::js/plugin/slick/slick.min.js") }}"></script>
<script src="{{ asset_path("theme::js/plugin/magnific-popup/jquery.magnific-popup.min.js") }}"></script>
<script src="{{ asset_path("theme::js/plugin/mixitup/mixitup.min.js") }}"></script>
<script src="{{ asset_path("theme::js/plugin/SyoTimer/jquery.syotimer.min.js") }}"></script>
<script src="{{ asset_path("theme::js/plugin/google-map/map.js") }}"></script>
<script src="{{ asset_path("theme::js/plugin/masonry/masonry2.pkgd.min.js") }}"></script>



{# Theme Scripts #}
{#{{ asset_add("theme.js", "theme::js/plugins/*") }}#}
{{ asset_add("theme.js", "theme::js/theme/initialize.js") }}

{{ html_script(asset_path("theme.js", ["min"])) }}

{# Include Scripts Loaded From Outside #}
{% for script in asset_scripts("scripts.js") %}
    {{ script|raw }}
{% endfor %}

{# Include Styles Loaded From Outside #}
{% for style in asset_styles("styles.css") %}
    {{ style|raw }}
{% endfor %}
